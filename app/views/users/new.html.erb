<% title 'Register for All Your Cells' %>
 
<% semantic_form_for @user, :url => account_path do |f| %>
  <% flash[:error] = f.error_messages %>
  <%= render :partial => "form", :locals  => {:f => f} %>
  <% f.buttons do %>
    <%= f.commit_button "Register &raquo;",  :button_html => { :class => 'awesome orange' } %>
  <% end %>
<% end %>